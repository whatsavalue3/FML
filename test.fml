local const u16* screen = 0xf800;

local const u8* input = 0xf046;

local const u16 theb = 0xb;

function u16 Getthing()
	return (*screen) + 0xff90 + theb;
end

local u16 i = 0;
local u16 j = 0;

function void main()
	while 1 do
		i = 0;
		while 0x7fe > i do
			*(screen + i) = j;
			i = i + 1;
			j = j + 1;
		end
	end
end